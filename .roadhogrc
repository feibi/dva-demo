{
  "entry": "src/index.js",
  "disableCSSModules": false,
  "autoprefixer": null,
  "proxy": {
    "/api/users*": {
      "target": "http://jsonplaceholder.typicode.com/",
      "changeOrigin": true,
      "secure": false,
       "pathRewrite": {"^/api" : ""}
    },
    "/api/v1/": {
      "target": "http://10.188.10.2/",
      "changeOrigin": true,
      "secure": false
    }
  },
  "extraBabelPlugins": [
    "transform-runtime", [
      "import", {
        "libraryName": "antd",
        "style": "css"
      }
    ]
  ],
  "env": {
    "development": {
      "extraBabelPlugins": [
        "dva-hmr",
        "transform-runtime"
      ]
    },
    "production": {
      "extraBabelPlugins": [
        "transform-runtime"
      ]
    }
  }
}
